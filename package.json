{
    "name": "react-template",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@testing-library/jest-dom": "^5.11.9",
        "@testing-library/react": "^11.2.5",
        "@testing-library/react-hooks": "^5.0.3",
        "@testing-library/user-event": "^12.7.1",
        "axios": "^0.21.1",
        "bootstrap": "^4.6.0",
        "connected-react-router": "^6.8.0",
        "express": "^4.17.1",
        "history": "^5.0.0",
        "json-server": "^0.16.3",
        "nodemon": "^2.0.7",
        "path": "^0.12.7",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-redux": "^7.2.2",
        "react-router": "^5.2.0",
        "react-router-dom": "^5.2.0",
        "react-scripts": "4.0.2",
        "redux": "^4.0.5",
        "redux-saga": "^1.1.3",
        "web-vitals": "^1.0.1"
    },
    "scripts": {
        "local-start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "test-coverage": "react-scripts test --coverage",
        "test-update": "react-scripts test --u",
        "eject": "react-scripts eject",
        "apiserver": "json-server --watch --port 4000 api/db.json --routes routes.json",
        "start": "npm run build && NODE_ENV=prod && nodemon server.js && start http://localhost:3001"
    },
    "jest": {
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
        ],
        "coveragePathIgnorePatterns": [
            "src/Api",
            "src/*index.js"
        ],
        "collectCoverageFrom": [
            "**/*.{js,jsx}",
            "!**/node_modules/**",
            "!**/*index.js",
            "!**/*actions.js"
        ]
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
